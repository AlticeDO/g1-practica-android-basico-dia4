<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.orange.ejemplo.samplesd4">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MyTextViewActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MyEditTextActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MyAutoCompleteTextViewActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MyButtonActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MyImageButtonActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MyCheckboxActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MyToggleButtonActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MyRadioButtonActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MyRadioGroupActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MyProgressBarActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MySpinnerActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MyTimePickerActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MyDatePickerActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MyImageViewActivity"
            android:parentActivityName=".MainActivity" />
    </application>

</manifest>